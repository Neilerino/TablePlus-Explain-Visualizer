{
  "name": "tableplus-postgres-explain",
  "version": "2.0.0",
  "description": "PostgreSQL EXPLAIN Visualizer for TablePlus",
  "type": "module",
  "main": "dist/explain.js",
  "scripts": {
    "build": "npm run build:plugin && npm run build:app",
    "build:plugin": "webpack --mode production --config webpack.config.cjs",
    "build:app": "webpack --mode production --config app.webpack.config.cjs",
    "dev": "webpack --mode development --watch --config webpack.config.cjs",
    "dev:app": "webpack serve --config app.webpack.config.cjs",
    "clean": "rm -rf dist/* PostgresExplain.tableplusplugin/app/dist/*",
    "prebuild": "npm run clean",
    "postbuild:plugin": "cp dist/explain.js PostgresExplain.tableplusplugin/",
    "copy-to-plugin": "cp dist/explain.js PostgresExplain.tableplusplugin/",
    "test-browser": "node test-browser.js"
  },
  "keywords": [
    "tableplus",
    "postgresql",
    "explain",
    "visualization",
    "d3"
  ],
  "author": "TablePlus",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^6.11.0",
    "esbuild": "^0.27.2",
    "html-webpack-plugin": "^5.6.5",
    "mini-css-extract-plugin": "^2.9.4",
    "raw-loader": "^4.0.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "highlight.js": "^11.11.1"
  }
}
